<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>

<ul>
  <% @products.each do |product| %>
    <h3>Product Name</h3>
    <li><%= product.name %></li>
    <h3>Product Description</h3>
    <li><%= product.description %></li>
    <h3>Product City</h3>
    <li><%= product.city %></li>
    <h3>Product Price</h3>
    <li><%= product.price %></li>
    <hr>
  <% end %>
</ul>
